{
    "overrides": [
      {
        "files": ["**.ts", "**.js", "**.yml"],
        "options": {
          "tabWidth": 2,
          "useTabs": false,
          "endOfLine": "lf",
          "printWidth": 120,
          "singleQuote": true
        }
      },
      {
        "files": "**.json",
        "options": {
          "tabWidth": 2,
          "printWidth": 180
        }
      }
    ]
  }