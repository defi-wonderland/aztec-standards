# Aztec Deployment Configuration
# Copy this file to .env and customize for local development

# Aztec Network Configuration
AZTEC_RPC_URL=https://aztec-testnet.aztec.network:8080
AZTEC_ACCOUNT_ALIAS=deployer

# Deployment Configuration
MAX_RETRIES=5
RETRY_DELAY=5000

# Optional: Custom deployment settings
# DEPLOYMENT_ENVIRONMENT=testnet
# LOG_LEVEL=info

# Note: Do not commit sensitive information to version control
# The .env file should be added to .gitignore
